{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "class-methods-use-this": ["off"],
        "import/prefer-default-export": ["off"]
    },
    "env": {
        "node": true,
        "mocha": true
    }
}